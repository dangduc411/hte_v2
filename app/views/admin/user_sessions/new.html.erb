        <div class="col-lg-12">
          <h1 class="page-header">Login</h1>
        </div>
      </div>
	    <% flash.each do |type, message| %>
	      <div class="alert alert-<%= type %> fade in">
	        <button class="close" data-dismiss="alert">Ã—</button>
